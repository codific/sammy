{
  "name" : "codific/sammy",
  "license" : "CC BY-SA 4.0",
  "type" : "project",
  "description" : "SAMMY v2",
  "require" : {
    "php" : "^8.2",
    "ext-ctype" : "*",
    "ext-curl" : "*",
    "ext-dom" : "*",
    "ext-fileinfo" : "*",
    "ext-iconv" : "*",
    "ext-json" : "*",
    "ext-openssl" : "*",
    "ext-simplexml" : "*",
    "ext-zip" : "*",
    "composer/package-versions-deprecated" : "1.*",
    "doctrine/annotations" : "^2.0",
    "doctrine/doctrine-bundle" : "2.*",
    "doctrine/doctrine-migrations-bundle" : "3.*",
    "doctrine/migrations" : "3.*",
    "doctrine/orm" : "3.*",
    "egulias/email-validator" : "4.*",
    "endroid/qr-code-bundle" : "5.*",
    "google/apiclient" : "2.*",
    "guzzlehttp/guzzle" : "7.*",
    "league/commonmark" : "^2.4",
    "phpdocumentor/reflection-docblock" : "5.*",
    "phpmailer/phpmailer" : "6.*",
    "phpoffice/phpspreadsheet" : "*",
    "predis/predis" : "^2.2",
    "scheb/2fa-backup-code" : "7.*",
    "scheb/2fa-bundle" : "7.*",
    "scheb/2fa-google-authenticator" : "7.*",
    "scheb/2fa-trusted-device" : "7.*",
    "scienta/doctrine-json-functions" : "*",
    "symfony/apache-pack" : "*",
    "symfony/asset" : "*",
    "symfony/console" : "*",
    "symfony/css-selector" : "*",
    "symfony/dom-crawler" : "*",
    "symfony/dotenv" : "*",
    "symfony/expression-language" : "*",
    "symfony/flex" : "*",
    "symfony/form" : "*",
    "symfony/framework-bundle" : "*",
    "symfony/html-sanitizer" : "*",
    "symfony/http-client" : "*",
    "symfony/intl" : "*",
    "symfony/mime" : "*",
    "symfony/monolog-bundle" : "*",
    "symfony/notifier" : "*",
    "symfony/process" : "*",
    "symfony/property-access" : "*",
    "symfony/property-info" : "*",
    "symfony/proxy-manager-bridge" : "*",
    "symfony/runtime" : "*",
    "symfony/security-bundle" : "*",
    "symfony/serializer" : "*",
    "symfony/string" : "*",
    "symfony/translation" : "*",
    "symfony/twig-bundle" : "*",
    "symfony/validator" : "*",
    "symfony/web-link" : "*",
    "symfony/yaml" : "*",
    "twig/extra-bundle" : "3.*",
    "twig/markdown-extra" : "3.*",
    "twig/string-extra" : "3.*",
    "twig/twig" : "3.*"
  },
  "require-dev" : {
    "dama/doctrine-test-bundle" : "*",
    "friendsofphp/php-cs-fixer" : "^3.23",
    "jetbrains/phpstorm-attributes" : "1.*",
    "phpstan/extension-installer" : "1.*",
    "phpstan/phpstan" : "1.*",
    "phpstan/phpstan-deprecation-rules" : "^1.2",
    "phpstan/phpstan-doctrine" : "1.*",
    "phpstan/phpstan-strict-rules" : "^1.6",
    "phpstan/phpstan-symfony" : "^1.2",
    "phpunit/phpunit" : "9.*",
    "psr/log" : "^2",
    "spaze/phpstan-disallowed-calls" : "*",
    "squizlabs/php_codesniffer" : "*",
    "symfony/browser-kit" : "*",
    "symfony/debug-bundle" : "*",
    "symfony/maker-bundle" : "*",
    "symfony/panther" : "^2.0",
    "symfony/phpunit-bridge" : "*",
    "symfony/stopwatch" : "*",
    "symfony/web-profiler-bundle" : "*"
  },
  "config" : {
    "preferred-install" : {
      "*" : "dist"
    },
    "sort-packages" : true,
    "allow-plugins" : {
      "composer/package-versions-deprecated" : true,
      "endroid/installer" : true,
      "symfony/flex" : true,
      "symfony/runtime" : true,
      "phpstan/extension-installer" : true
    }
  },
  "autoload" : {
    "psr-4" : {
      "App\\" : "src/"
    }
  },
  "autoload-dev" : {
    "psr-4" : {
      "App\\Tests\\" : "tests/"
    }
  },
  "replace" : {
    "paragonie/random_compat" : "2.*",
    "symfony/polyfill-ctype" : "*",
    "symfony/polyfill-iconv" : "*",
    "symfony/polyfill-php72" : "*",
    "symfony/polyfill-php71" : "*",
    "symfony/polyfill-php70" : "*",
    "symfony/polyfill-php56" : "*"
  },
  "scripts" : {
    "phpstan" : "vendor/bin/phpstan --ansi analyse --memory-limit=4G --configuration phpstan.neon",
    "auto-scripts" : {
      "cache:clear" : "symfony-cmd",
      "assets:install %PUBLIC_DIR%" : "symfony-cmd"
    },
    "yarn" : [
      "cd public/front && yarn install",
      "cd public/shared && yarn install"
    ],
    "post-install-cmd" : [
      "@auto-scripts",
      "@yarn"
    ],
    "post-update-cmd" : [
      "@auto-scripts",
      "@yarn"
    ],
    "phpcs" : "phpcs --standard=PSR12",
    "php-cs-fixer" : "php-cs-fixer --config=./.php-cs-fixer.dist"
  },
  "conflict" : {
    "symfony/symfony" : "*"
  },
  "extra" : {
    "symfony" : {
      "allow-contrib" : false,
      "require" : "7.2.*"
    }
  }
}
